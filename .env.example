# Kali Agents Configuration Template
# Copy this file to .env and adjust values as needed

# MCP Server Configuration
DATA_SERVER_HOST=localhost
DATA_SERVER_PORT=5000
NETWORK_SERVER_PORT=5001
WEB_SERVER_PORT=5002
VULN_SERVER_PORT=5003
FORENSIC_SERVER_PORT=5004
SOCIAL_SERVER_PORT=5005
REPORT_SERVER_PORT=5006
MCP_TRANSPORT=http

# LLM Configuration
LLM_MODEL=llama3.2
LLM_HOST=localhost
LLM_PORT=11434

# Kali Tools Paths (adjust based on your Kali installation)
NMAP_PATH=/usr/bin/nmap
GOBUSTER_PATH=/usr/bin/gobuster
SQLMAP_PATH=/usr/bin/sqlmap
NIKTO_PATH=/usr/bin/nikto
DIRB_PATH=/usr/bin/dirb
MASSCAN_PATH=/usr/bin/masscan
SEARCHSPLOIT_PATH=/usr/bin/searchsploit

# Wordlist Paths
WORDLIST_COMMON=/usr/share/wordlists/dirb/common.txt
WORDLIST_BIG=/usr/share/wordlists/dirb/big.txt
WORDLIST_ROCKYOU=/usr/share/wordlists/rockyou.txt

# Metasploit Configuration
MSF_RPC_HOST=localhost
MSF_RPC_PORT=55553
MSF_RPC_USER=msf
MSF_RPC_PASS=CHANGE_ME

# Burp Suite Pro API (if available)
BURP_API_HOST=localhost
BURP_API_PORT=1337
BURP_API_KEY=CHANGE_ME

# Report Configuration
REPORT_OUTPUT_DIR=./reports
REPORT_TEMPLATE_DIR=./templates
COMPANY_NAME=Your Company
PENTESTER_NAME=Your Name

# Security Configuration
API_KEY_SECRET=CHANGE_ME
JWT_SECRET=CHANGE_ME

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/kali-agents.log

# Database Configuration (for storing scan results)
DB_TYPE=sqlite
DB_PATH=./data/kali-agents.db

# Network Configuration
DEFAULT_TIMEOUT=30
MAX_CONCURRENT_SCANS=5
RATE_LIMIT_DELAY=1

# OSINT Configuration
SHODAN_API_KEY=CHANGE_ME
VIRUSTOTAL_API_KEY=CHANGE_ME
CENSYS_API_ID=CHANGE_ME
CENSYS_API_SECRET=CHANGE_ME

# Social Engineering Configuration
SET_CONFIG_PATH=/etc/setoolkit/set.config
GOPHISH_API_KEY=CHANGE_ME
GOPHISH_URL=https://localhost:3333

# Development Configuration
DEBUG=false
DEVELOPMENT_MODE=false
